    "version": "0.2.0",
    "configurations": [
      {
        "name": "(gdb) Launch",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/app",
        "stopAtEntry": true,
        "cwd": "${workspaceFolder}",
        "externalConsole": false,
        "MIMode": "gdb",
        "miDebuggerServerAddress": "192.168.0.14:1234",
        "miDebuggerPath": 
          "/home/lmi/workspace/nano/buildroot/output/host/usr/bin/aarch64-none-linux-gnu-gdb"
      }
    ]
  }
  
